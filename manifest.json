{
  "manifest_version": 2,

  "name": "siteSmash",
  "description": "This extension can be used to rate websites",
  "version": "0.5",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
